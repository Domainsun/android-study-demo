<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



    <Button
        android:id="@+id/btn_of_int"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="ofInt"
        android:textAllCaps="false"></Button>

    <Button
        android:id="@+id/btn_of_float"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="ofFloat"
        android:textAllCaps="false"></Button>

<Button
        android:id="@+id/btn_of_rgb"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="ofRGB"
    android:textAllCaps="false"></Button>


    <Button
        android:id="@+id/btn_of_object"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="ofObject"
        android:textAllCaps="false"></Button>

       <Button
        android:id="@+id/btn_of_values_property"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="ofValuesProperty"
        android:textAllCaps="false"></Button>



    <com.domain.study.animation.property.MyView
        android:id="@+id/myView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
       >

    </com.domain.study.animation.property.MyView>
</LinearLayout>
